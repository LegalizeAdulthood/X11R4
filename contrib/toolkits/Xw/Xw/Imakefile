    STD_DEFINES = LibraryDefines
    CDEBUGFLAGS = LibraryCDebugFlags
       INCLUDES = -I.. -I$(TOP)
   INSTALLFLAGS = $(INSTINCFLAGS)
       LINTLIBS = $(LINTXLIB) $(LINTXTOOL)
# We have a link from /usr/include/Xw to $(LIBDIR)/Xw. You may prefer to 
# just install it there.
       XWINCDIR = $(LIBDIR)/Xw


