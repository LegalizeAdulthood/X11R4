HDRSPUBLIC=     str_utils.h
HDRSPRIVATE=
HEADERS= ${HDRSPUBLIC} ${HDRSPRIVATE}
INCLUDES= -I$(HEADER_DEST) -I$(TOP)

OBJS =\
	str_utils.o\
	whitespace.o

SRCS =\
	str_utils.c\
	whitespace.c

all::

.c.o:
	$(RM) $@
	$(CC) -c $(CFLAGS) $*.c

all:: libxvin.a

libxvin.a: $(OBJS)
	$(AR) ../$@ $(OBJS)

all:: libxview.a

libxview.a:  $(OBJS)
	$(AR) ../../$@  $(OBJS)

install_headers:: $(HDRSPUBLIC)
	@case '${MFLAGS}' in *[i]*) set +e;; esac; \
	for i in $(HDRSPUBLIC); do \
	(set -x; $(INSTALL) -c $(INSTALLFLAGS) $$i $(HEADER_DEST)/xview); \
	done

depend:: $(DEPEND)

$(DEPEND):
	@echo "checking $@ over in $(DEPENDSRC) first..."; \
	cd $(DEPENDSRC); $(MAKE); \
	echo "okay, continuing in $(CURRENT_DIR)"

depend::
	$(DEPEND) -s "# DO NOT DELETE" -- $(ALLDEFINES) -- $(SRCS)

