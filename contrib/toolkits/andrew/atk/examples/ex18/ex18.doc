static void setSubView(hwv,dobj)

struct helloworldview *hwv;

struct dataobject *dobj;

\{

    if(hwv->view!=NULL)\{

	view_UnlinkTree(hwv->applayer);

	if(hwv->view!=hwv->applayer)

	    view_DeleteApplicationLayer(hwv->view,hwv->applayer);

