all:: dicserver

dicserver: $(OBJS) $(LOCAL_LIBRARIES)
	$(RM) $@
	$(CC) -o $@ $(OBJS) $(LOCAL_LIBRARIES) $(LDFLAGS)  $(SYSLAST_LIBRARIES)

relink::
	$(RM) dicserver
	$(MAKE) $(MFLAGS) dicserver

clean::
	$(RM) dicserver

install:: dicserver
	$(INSTALL) -c $(INSTALLFLAGS) dicserver $(WNNBINDIR)

depend:: $(DEPEND)

depend::
	$(DEPEND) -s "# DO NOT DELETE" -- $(CFLAGS) -- $(SRCS)

$(DEPEND):
	@echo "making $@"; \
	cd $(DEPENDSRC); $(MAKE)

install::
	strip $(WNNBINDIR)/dicserver

$(OBJ2) : $(SRC2)
	$(CC) -c $(CFLAGS) $(FEXWNMOSRC)/`basename $@ .o`.c

$(OBJ3) : $(SRC3)
	$(CC) -c $(CFLAGS) $(WNNETCSRC)/`basename $@ .o`.c

$(OBJ4) : $(SRC4)
	$(CC) -c $(CFLAGS) $(WNNETCSRC)/`basename $@ .o`.c

$(WNNLIB):
	@echo Please make $(WNNLIB)
	@false

