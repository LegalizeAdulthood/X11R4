			tags TAGS make.log

LOCAL_LIBRARIES = $(XLIB)
OBJS = bitimp.o readump.o swap.o

 PROGRAM = bitimp

all:: bitimp

bitimp: $(OBJS) $(LOCAL_LIBRARIES)
	$(RM) $@
	$(CC) -o $@ $(OBJS) $(LOCAL_LIBRARIES) $(LDFLAGS)

relink::
	$(RM) $(PROGRAM)
	$(MAKE) $(MFLAGS) $(PROGRAM)

install:: bitimp
	$(INSTALL) -c $(INSTALLFLAGS) bitimp $(BINDIR)

install:: bitimp.man
	$(INSTALL) -c $(INSTMANFLAGS) bitimp.man $(MANDIR)/bitimp.1

depend:: $(DEPEND)

depend::
	$(DEPEND) -s "# DO NOT DELETE" $(CFLAGS) $(SRCS)

$(DEPEND):
	@echo "making $(DEPENDSRC)"; \
	cd $(DEPENDSRC); $(MAKE)

clean::
	$(RM) $(PROGRAM)

all:: xipr

xipr: xipr.sh
	cp xipr.sh xipr

install:: xipr
	$(INSTALL) -c $(INSTALLFLAGS) xipr $(BINDIR)

install:: xipr.man
	$(INSTALL) -c $(INSTMANFLAGS) xipr.man $(MANDIR)/xipr.1

clean::
	$(RM_CMD) \#*

Makefile:: $(IMAKE)

Makefile:: Imakefile \
	$(IRULESRC)/Imake.tmpl \
	$(IRULESRC)/Imake.rules \
	$(IRULESRC)/$(MACROFILE)
	-$(RM) Makefile.bak; $(MV) Makefile Makefile.bak
	$(IMAKE_CMD) -DTOPDIR=$(TOP)

$(IMAKE):
	@echo "making $(IMAKESRC)"; \
	cd $(IMAKESRC); $(MAKE)

tags::
	$(TAGS) -w *.[ch]
	$(TAGS) -xw *.[ch] > TAGS

install::
	@echo "install done"

