FONTINSTDIR = $(FONTDIR)/xtroff

all: $(SRCS) $(OBJS) fonts.dir

$(SRCS):
	rm -f $@
	$(LN) $(FONTSRC)/bdf/75dpi/$@

clean::
	rm -f *.snf.Z *.bdf

fonts.dir: $(OBJS)
	$(MKFONTDIR) .

install::
	$(MKDIRHIER)  $(FONTINSTDIR)

install:: $(OBJS)
	@case '${MFLAGS}' in *[i]*) set +e;; esac; \
	for i in $(OBJS); do \
	(set -x; $(INSTALL) -c $(INSTALLFLAGS) $$i $(FONTINSTDIR)); \
	done

install:: fonts.dir
	$(INSTALL) -c $(INSTINCFLAGS) fonts.dir $(FONTINSTDIR)

.SUFFIXES: .bdf .snf.Z .t.snf.Z

.bdf.snf.Z:
	rm -f $*.snf $*.snf.Z
	$(FONTC) $< > $*.snf
	compress $*.snf

.bdf.t.snf.Z:
	rm -f $*.t.snf $*.t.snf.Z
	awk -f adobe.awk $< | $(FONTC) > $*.t.snf
	compress $*.t.snf

