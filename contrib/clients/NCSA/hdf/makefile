# Imakefile for creating libdf.a, the Hierarchical Data Format (HDF)
# library and miscellaneous HDF utilities.
#
# Check with anonymous ftp "ftp.ncsa.uiuc.edu" or "128.174.20.50" for the
# latest versions with new features.  Questions?   Call (217) 244-0647 or
# write to NCSA, 605 East Springfield Ave., Champaign, IL 61820.
###########################################################################

CC = cc
AR = ar
RM = /bin/rm -f
RANLIB = ranlib
HDFLIB = libdf.a
GLIB  = -lpixrect
OBJS= df.o dfr8.o dfsd.o dfgroup.o dfcomp.o dfimcomp.o  \
	dfkit.o dfp.o dfan.o  dfgr.o df24.o
UTILS  = hdfrseq hdfls r8tohdf hdfcomp hdftotek tektohdf hdftor8
DEFS   = -g
CFLAGS = ${DEFS}
ARFLAGS = r

MACHINE=SUN

.c.o:
	$(RM) $@
	$(CC) -c $(CFLAGS) $*.c

$(HDFLIB): $(OBJS)
	$(RM) $@
	$(AR) $(ARFLAGS) $@ $(OBJS)
	$(RANLIB) $@

hdfrseq:  hdfrseq.o  libdf.a
	$(RM) $@
	$(CC) -o $@  hdfrseq.o  libdf.a $(LDFLAGS) $(GLIB) $(SYSLAST_LIBRARIES)

relink::
	$(RM)  hdfrseq
	$(MAKE) $(MFLAGS)  hdfrseq

clean::
	$(RM)  hdfrseq

hdfls:  		hdfls.o  	$(HDFLIB)
	$(RM) $@
	$(CC) -o $@  		hdfls.o  	$(HDFLIB) $(LDFLAGS)   $(SYSLAST_LIBRARIES)

relink::
	$(RM)  hdfls
	$(MAKE) $(MFLAGS)  hdfls

clean::
	$(RM)  hdfls

r8tohdf:  	r8tohdf.o 	$(HDFLIB)
	$(RM) $@
	$(CC) -o $@  	r8tohdf.o 	$(HDFLIB) $(LDFLAGS)   $(SYSLAST_LIBRARIES)

relink::
	$(RM)  r8tohdf
	$(MAKE) $(MFLAGS)  r8tohdf

clean::
	$(RM)  r8tohdf

hdfcomp:  	hdfcomp.o  	$(HDFLIB)
	$(RM) $@
	$(CC) -o $@  	hdfcomp.o  	$(HDFLIB) $(LDFLAGS)   $(SYSLAST_LIBRARIES)

relink::
	$(RM)  hdfcomp
	$(MAKE) $(MFLAGS)  hdfcomp

clean::
	$(RM)  hdfcomp

hdftotek:  	hdftotek.o  $(HDFLIB)
	$(RM) $@
	$(CC) -o $@  	hdftotek.o  $(HDFLIB) $(LDFLAGS)   $(SYSLAST_LIBRARIES)

relink::
	$(RM)  hdftotek
	$(MAKE) $(MFLAGS)  hdftotek

clean::
	$(RM)  hdftotek

tektohdf: 	tektohdf.o  $(HDFLIB)
	$(RM) $@
	$(CC) -o $@ 	tektohdf.o  $(HDFLIB) $(LDFLAGS)   $(SYSLAST_LIBRARIES)

relink::
	$(RM)  tektohdf
	$(MAKE) $(MFLAGS)  tektohdf

clean::
	$(RM)  tektohdf

hdftor8:  	hdftor8.o  	$(HDFLIB)
	$(RM) $@
	$(CC) -o $@  	hdftor8.o  	$(HDFLIB) $(LDFLAGS)   $(SYSLAST_LIBRARIES)

relink::
	$(RM)  hdftor8
	$(MAKE) $(MFLAGS)  hdftor8

clean::
	$(RM)  hdftor8

all:: $(HDFLIB) $(UTILS)

